create table myloginlog(
num number constraint myloginlogs_num_pk primary key,
idn varchar2(10),
reip varchar2(10),
uagent varchar2(100),
status varchar2(10),
sstime date default sysdate,
eetime date default sysdate,
constraint myloginlogs_idn_fk foreign key(idn)
references mymember(id)
);
create sequence myloginlogs
increment by 1 
start with 1;